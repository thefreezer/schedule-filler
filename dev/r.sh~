#!/bin/bash

curl -v --compressed --cookie-jar resCookies --cookie cookies\
     -H "Host: www.beartracks.ualberta.ca" \
     -H "User-Agent: Mozilla/5.0 (X11; Linux x86_64; rv:74.0) Gecko/20100101 Firefox/74.0" \
     -H "Accept: */*" \
     -H "Accept-Language: en-CA,en-US;q=0.7,en;q=0.3" \
     -H "Accept-Encoding: gzip, deflate, br" \
     -H "Content-Type: application/x-www-form-urlencoded" \
     -H "Content-Length: 516" \
     -H "Origin: https://www.beartracks.ualberta.ca" \
     -H "Connection: keep-alive" \
     -H "Referer: https://www.beartracks.ualberta.ca/psc/uahegprd/EMPLOYEE/HRMS/c/COMMUNITY_ACCESS.CLASS_SEARCH.GBL?Page=SSR_CLSRCH_ENTRY&Action=U" \
     -d @c.txt \
     https://www.beartracks.ualberta.ca/psc/uahegprd/EMPLOYEE/HRMS/c/COMMUNITY_ACCESS.CLASS_SEARCH.GBL

